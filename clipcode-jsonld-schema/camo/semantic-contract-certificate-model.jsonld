{
  "@context": {
    "xsd":    "http://www.w3.org/2001/XMLSchema#",
    "rdf":    "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs":   "http://www.w3.org/2000/01/rdf-schema#",
    "schema": "https://schema.org/",

    "coremo": "https://clipcode.org/base/schema/coremo/",
    "camo":   "https://clipcode.org/base/schema/camo/"
  },
  "@graph": [
    {
      "@id": "camo:ContractCertificate",
      "@type": "rdfs:Class",
      "rdfs:comment": "A certificate for a contract.",
      "rdfs:label": "ContractCertificate",
      "rdfs:subClassOf": [
        { "@id": "camo:CertificateContractConcept" },
        { "@id": "coremo:Numbered" },
        { "@id": "camo:ContractCommunication" }
      ]
    },
    {
      "@id": "camo:hasCertificate",
      "@type": "rdf:Property",
      "rdfs:comment": "Attaches a certificate to its semantic pool.",
      "rdfs:label": "hasRootCertificateContractConcept",
      "schema:domainIncludes": { "@id": "camo:CertificateContractSemanticPool" },
      "schema:rangeIncludes":  { "@id": "camo:ContractCertificate" }
    },  
    {
      "@id": "camo:contractCertificateKind",
      "@type": "rdf:Property",
      "rdfs:comment": "Identifies what kind of certificate this is (useful for specialist certificates with custom names, maps to certfiicate design in ContractDesign).",
      "rdfs:label": "contractCertificateKind",
      "schema:domainIncludes": { "@id": "camo:ContractCertificate" },
      "schema:rangeIncludes":  { "@id": "schema:URL" }
    },
    {
      "@id": "camo:certificateBasedOnContractDataRevision",
      "@type": "rdf:Property",
      "rdfs:comment": "Most recent core contract data revision.",
      "rdfs:label": "certificateBasedOnCoreContractDataRevision",
      "schema:domainIncludes": { "@id": "camo:ContractCertificate" },
      "schema:rangeIncludes":  { "@id": "schema:Integer" }
    }
  ]
}
