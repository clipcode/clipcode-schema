{
  "@context": {
    "xsd":    "http://www.w3.org/2001/XMLSchema#",
    "rdf":    "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs":   "http://www.w3.org/2000/01/rdf-schema#",
    "schema": "https://schema.org/",

    "coremo": "https://clipcode.org/base/schema/coremo/",
    "camo":   "https://clipcode.org/base/schema/camo/"
  },
  "@graph": [
    {
      "@id": "camo:RegularCertificate",
      "@type": "rdfs:Class",
      "rdfs:comment": "A regular certificate for a contract.",
      "rdfs:label": "RegularCertificate",
      "rdfs:subClassOf": { "@id": "camo:RegularCertificateContractConcept" }
    },
    {
      "@id": "camo:hasRegularCertificate",
      "@type": "rdf:Property",
      "rdfs:comment": "Attaches a regular certificate to its semantic pool.",
      "rdfs:label": "hasRegularCertificate",
      "schema:domainIncludes": { "@id": "camo:RegularCertificateContractSemanticPool" },
      "schema:rangeIncludes":  { "@id": "camo:RegularCertificate" }
    },  
    {
      "@id": "camo:contractRegularCertificateKind",
      "@type": "rdf:Property",
      "rdfs:comment": "Identifies what kind of certificate this is.",
      "rdfs:label": "contractRegularCertificateKind",
      "schema:domainIncludes": { "@id": "camo:RegularCertificate" },
      "schema:rangeIncludes":  { "@id": "schema:URL" }
    },
    {
      "@id": "camo:PerformanceCertificate",
      "@type": "rdfs:Class",
      "rdfs:comment": "A performance certificate for a contract.",
      "rdfs:label": "PerformanceCertificate",
      "rdfs:subClassOf": { "@id": "camo:RegularCertificate" }
    },
    {
      "@id": "camo:TakingOverCertificate",
      "@type": "rdfs:Class",
      "rdfs:comment": "A taking-over certificate for a contract.",
      "rdfs:label": "TakingOverCertificate",
      "rdfs:subClassOf": { "@id": "camo:RegularCertificate" }
    },
    {
      "@id": "camo:MilestoneCertificate",
      "@type": "rdfs:Class",
      "rdfs:comment": "A milestone certificate for a contract.",
      "rdfs:label": "MilestoneCertificate",
      "rdfs:subClassOf": { "@id": "camo:RegularCertificate" }
    }
  ]
}
