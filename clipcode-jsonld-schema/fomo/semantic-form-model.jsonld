    {
      "@id": "fomo:equalsOrLessConditionalOp",
      "@type": "fomo:ConditionalOperationEnumeration",
      "rdfs:comment": "equalsOrLess conditional op.",
      "rdfs:label": "eualsOrLessConditionalOp"
